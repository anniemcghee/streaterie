


<br>
<br>
<div class='container'>
  <div class='row table'>
    <div class='col-md-4'><h2>Cuisine</h2></div>
    <div class='col-md-5 '><h2><span class='glyphicon glyphicon-tags glyphicon-lg'></span></h2></div>
    <div class='col-md-2 text-center'><h2>Remove</h2></div>
  </div>
  <% @cuisines.each do |cuisine| %>
  <div class='row table'>
    <div class='col-md-4'>
      <%= cuisine.name %>
    </div>
    <div class='col-md-5'>
      <% cuisine.tags.each do |tag| %>
        <%= link_to tag.name, tag_path(tag.name) %>
      <% end %>
    </div>
    <div class='col-md-2 text-center'><span class='glyphicon glyphicon-remove'></span></div>
  </div>
  <% end %>
</div>